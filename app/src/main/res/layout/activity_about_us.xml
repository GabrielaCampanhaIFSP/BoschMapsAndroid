        android:text="The BOSCH MAPS application was\ncreated to help new and old employees\nfind their way around the bosch\nCampinas region, bring information\nabout each building and even some information about the restaurant.\nWe hope you enjoy it!"
        android:textColor="#FFFFFF"
        android:textSize="19dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
